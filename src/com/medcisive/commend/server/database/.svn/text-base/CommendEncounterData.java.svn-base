public class CommendEncounterData
{

    public String m_SSN;
    public String m_TrtLocIEN;
    public Timestamp m_apptDateTime;

    public String m_apptStatus;
    public String m_cancellationReason;

    public CommendEncounterData(String ssn, String trtLocIEN, 
                                Timestamp apptDateTime, String apptStatus, String cancReason) {
      m_SSN = ssn;
      m_TrtLocIEN = trtLocIEN;
      m_apptDateTime = apptDateTime;
      m_apptStatus = apptStatus;
      m_cancellationReason = cancReason;
                                                               
    }                               

}
