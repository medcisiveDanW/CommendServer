                                                <td style="width: 20%;"><input type="radio" name="" value="4" disabled/>4</td>
                                                <td style="width: 20%;"><input type="radio" name="" value="5" disabled/>5</td>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Not at all</td>
                                                <td>A little</td>
                                                <td>Moderately</td>
                                                <td>Very</td>
                                                <td>Extremely</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div id="freqEditDivId" class="innerBlockLeft">
                                    <div class="innerBlockLeft" style="width: 100px;" ></div>
                                    <div class="innerBlockLeft">
                                        <input id="unitRadioHourEditId" class="unitsRadioClass" type="radio" name="unitsRadioEdit" value="Hour"  checked disabled/><label for="unitRadioHourEditId">Hour</label>
                                        <input id="unitRadioDayEditId"  class="unitsRadioClass" type="radio" name="unitsRadioEdit" value="Day" disabled/><label for="unitRadioDayEditId">Day</label>
                                        <input id="unitRadioWeekEditId" class="unitsRadioClass" type="radio" name="unitsRadioEdit" value="Week" disabled/><label for="unitRadioWeekEditId">Week</label>
