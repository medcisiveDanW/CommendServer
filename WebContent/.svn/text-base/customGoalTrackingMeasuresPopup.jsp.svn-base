                                        <input id="freqCreateRadioId" type="radio" class="createRadioClass" name="createRadioName"/><label for="freqCreateRadioId">Frequency</label>
                                        <input id="rangeCreateRadioId" type="radio" class="createRadioClass" name="createRadioName"/><label for="rangeCreateRadioId">Range</label>
                                    </div>
                                </div>
                            </div>
                            <div class="outterBlock" style=" height: 55px;">
                                <div id="likertCreateDivId" class="innerBlockLeft" style=" width: 100%;">
                                    <table>
                                        <thead>
                                            <tr>
                                                <td style="width: 20%;"><input type="radio" name="selectedRadioDisplayId" value="1" />1</td>
                                                <td style="width: 20%;"><input type="radio" name="selectedRadioDisplayId" value="2" />2</td>
                                                <td style="width: 20%;"><input type="radio" name="selectedRadioDisplayId" value="3" checked />3</td>
                                                <td style="width: 20%;"><input type="radio" name="selectedRadioDisplayId" value="4" />4</td>
                                                <td style="width: 20%;"><input type="radio" name="selectedRadioDisplayId" value="5" />5</td>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Not at all</td>
                                                <td>A little</td>
                                                <td>Moderately</td>
                                                <td>Very</td>
                                                <td>Extremely</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div id="freqCreateDivId" class="innerBlockLeft">
                                    <div class="innerBlockLeft" style="width: 100px;" ></div>
                                    <div class="innerBlockLeft">
                                        <input id="unitHourCreateRadioId" class="unitsRadioClass" type="radio" name="unitsRadioCreate" value="Hour"  checked/><label for="unitHourCreateRadioId">Hour</label>
                                        <input id="unitDayCreateRadioId"  class="unitsRadioClass" type="radio" name="unitsRadioCreate" value="Day"/><label for="unitDayCreateRadioId">Day</label>
                                        <input id="unitWeekCreateRadioId" class="unitsRadioClass" type="radio" name="unitsRadioCreate" value="Week"/><label for="unitWeekCreateRadioId">Week</label>
                                    </div>
                                </div>
